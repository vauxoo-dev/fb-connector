<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="invoice_1" model="account.invoice">
            <field name="currency_id" ref="base.EUR"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="journal_id" ref="commission_calculation.sales_journal"/>
            <field name="state">draft</field>
            <field name="type">out_invoice</field>
            <field name="account_id" ref="commission_calculation.a_recv"/>
            <field name="partner_id" ref="base.res_partner_12"/>
            <field eval="time.strftime('%Y-%m') + '-01'" name="date_invoice"/>
        </record>
        <record id="invoice_1_line_1" model="account.invoice.line">
            <field name="name">Vauxoo Specialties</field>
            <field name="invoice_id" ref="invoice_1"/>
            <field name="price_unit">2000</field>
            <field name="quantity">5</field>
            <field name="account_id" ref="commission_calculation.a_sale"/>
            <field name="product_id" ref="product.product_product_4"/>
        </record>
        <record id="invoice_1_line_2" model="account.invoice.line">
            <field name="name">VX Consultancy</field>
            <field name="invoice_id" ref="invoice_1"/>
            <field name="price_unit">4000</field>
            <field name="quantity">3</field>
            <field name="account_id" ref="commission_calculation.a_sale"/>
        </record>

        <record id="invoice_2" model="account.invoice">
            <field name="currency_id" ref="base.EUR"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="partner_id" ref="base.res_partner_1"/>
            <field name="journal_id" ref="commission_calculation.sales_journal"/>
            <field name="state">draft</field>
            <field name="type">out_invoice</field>
            <field name="account_id" ref="commission_calculation.a_recv"/>
            <field name="name">Test invoice 1</field>
        </record>
        <record id="invoice_2_line_1" model="account.invoice.line">
            <field name="name">Basic formation with Dvorak</field>
            <field name="invoice_id" ref="invoice_2"/>
            <field name="price_unit">500</field>
            <field name="quantity">1</field>
            <field name="account_id" ref="commission_calculation.a_sale"/>
        </record>
    </data>
</odoo>
